{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "dsfr/fr-accordion-title",
	"parent": ["dsfr/fr-accordion"],
	"version": "1.0.0",
	"title": "Accordion title",
	"category": "widgets",
	"icon": "editor-textcolor",
	"description": "DSFR accordion title block.",
	"supports": {
		"html": false,
		"anchor": false,
		"align": false,
		"lock": true,
		"reusable": false,
		"color": {
			"background": false,
			"text": false
		}
	},
	"attributes": {
		"label": {
			"type": "string",
			"default": ""
		},
		"ariaControls": {
			"type": "string",
			"default": ""
		}
	},
	"usesContext": ["dsfr/fr-accordion--id"],
	"textdomain": "dsfr-native-blocks",
	"editorScript": "file:./index.js"
}
